{
    "name": "orangephp/curl-to-cli",
    "description": "CurlToCLI is a Orange PHP, LLC, Project meant to convert standard `curl_exec()` calls to `curl` CLI commands.",
    "keywords": [
        "universal package layout",
        "standard package layout",
        "new project",
        "project layout"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Orange PHP, LLC.",
            "homepage": "https://www.orangephp.com/"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "0.0.0/composer-include-files": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*|9.*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "*",
        "symfony/var-dumper": "*"
    },
    "autoload": {
        "psr-4": {
            "OrangePHP\\CurlToCLI\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "OrangePHP\\CurlToCLI\\Tests\\" : "tests/" }
    },
    "extra": {
        "include_files": [
            "src/CurlToCLI.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "0.0.0/composer-include-files": true
        }
    }}
